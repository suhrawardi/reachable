.decl Edge(x:symbol, y:symbol)
.decl Reachable(x:symbol, y:symbol, n:number)

.input Edge(IO=file, filename="edges.csv", columns="0:1", delimiter=",")
.output Reachable(IO=file, filename="reachable.csv")

Reachable(x, y, 1) :- Edge(x, y).
Reachable(x, z, (n + 1)) :- Reachable(x, y, n), Edge(y, z).

